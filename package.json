{
  "name": "fakebrowser",
  "version": "0.0.19",
  "description": "",
  "main": "dist/cjs/FakeBrowser.js",
  "module": "dist/esm/FakeBrowser.js",
  "scripts": {
    "clean-dist": "rimraf dist",
    "build": "npm run tsc",
    "tsc": "npm run clean-dist && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc -b src/tsconfig.cjs.json",
    "tsc-esm": "tsc -b src/tsconfig.esm.json",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --port=7311 --ts",
    "postbuild": "cpy --cwd=src --parents plugins ../dist/cjs && cpy --cwd=src --parents plugins ../dist/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkoooqq/fakebrowser.git"
  },
  "keywords": [
    "bot",
    "crawler",
    "automation",
    "headless",
    "puppeteer",
    "puppeteer-extra",
    "puppeteer-extra-plugin",
    "puppeteer-extra-plugin-stealth",
    "scrapy",
    "fingerprint",
    "stealth",
    "anti-bot",
    "ua",
    "user-agent",
    "chrome"
  ],
  "author": "kkoooqqq",
  "license": "LGPLv3",
  "bugs": {
    "url": "https://github.com/kkoooqq/fakebrowser/issues"
  },
  "homepage": "https://github.com/kkoooqq/fakebrowser#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "express": "^4.17.1",
    "font-list": "^1.4.0",
    "fs-extra": "^10.0.0",
    "pidtree": "^0.5.0",
    "puppeteer": "^10.4.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin": "^3.2.0",
    "puppeteer-extra-plugin-user-preferences": "^2.3.1",
    "url-toolkit": "^2.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  }
}
